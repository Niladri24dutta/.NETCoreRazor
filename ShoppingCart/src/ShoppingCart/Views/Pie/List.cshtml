@model PiesListViewModel
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Niladri's Pie Shop</title>

</head>
<body>
    <h1>@Model.CurrentCategory</h1> &nbsp; 
    <h1>
        Total Items :@Model.Pies.ToList().Count      
    </h1>
    @foreach (var item in Model.Pies)
    {
        @Html.Partial("PieOverviewSummary",item)
    }
</body>
</html>